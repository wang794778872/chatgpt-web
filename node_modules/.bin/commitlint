#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/wsj/wework_app/chatgpt-web/node_modules/.pnpm/@commitlint+cli@17.4.4/node_modules/@commitlint/cli/node_modules:/home/ubuntu/wsj/wework_app/chatgpt-web/node_modules/.pnpm/@commitlint+cli@17.4.4/node_modules/@commitlint/node_modules:/home/ubuntu/wsj/wework_app/chatgpt-web/node_modules/.pnpm/@commitlint+cli@17.4.4/node_modules:/home/ubuntu/wsj/wework_app/chatgpt-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/wsj/wework_app/chatgpt-web/node_modules/.pnpm/@commitlint+cli@17.4.4/node_modules/@commitlint/cli/node_modules:/home/ubuntu/wsj/wework_app/chatgpt-web/node_modules/.pnpm/@commitlint+cli@17.4.4/node_modules/@commitlint/node_modules:/home/ubuntu/wsj/wework_app/chatgpt-web/node_modules/.pnpm/@commitlint+cli@17.4.4/node_modules:/home/ubuntu/wsj/wework_app/chatgpt-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@commitlint/cli/cli.js" "$@"
else
  exec node  "$basedir/../@commitlint/cli/cli.js" "$@"
fi
