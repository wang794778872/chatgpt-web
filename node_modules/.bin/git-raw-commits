#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/wsj/wework_app/chatgpt-web/node_modules/git-raw-commits/node_modules:/home/ubuntu/wsj/wework_app/chatgpt-web/node_modules:/home/ubuntu/wsj/wework_app/node_modules:/home/ubuntu/wsj/node_modules:/home/ubuntu/node_modules:/home/node_modules:/node_modules:/home/ubuntu/wsj/wework_app/chatgpt-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/wsj/wework_app/chatgpt-web/node_modules/git-raw-commits/node_modules:/home/ubuntu/wsj/wework_app/chatgpt-web/node_modules:/home/ubuntu/wsj/wework_app/node_modules:/home/ubuntu/wsj/node_modules:/home/ubuntu/node_modules:/home/node_modules:/node_modules:/home/ubuntu/wsj/wework_app/chatgpt-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../git-raw-commits/cli.js" "$@"
else
  exec node  "$basedir/../git-raw-commits/cli.js" "$@"
fi
